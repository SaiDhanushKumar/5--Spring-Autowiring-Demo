<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

 
    <bean id="laptop" class="com.klu.DependencyInjection.Laptop" />


    <bean id="student" class="com.klu.DependencyInjection.Student">
        <property name="laptop" ref="laptop" />
    </bean>


    <bean id="student1" class="com.klu.DependencyInjection.Student">
        <property name="registerNo" value="1001" />
        <property name="name" value="Hari" />
        <property name="laptop" ref="laptop" />
    </bean>

</beans>
